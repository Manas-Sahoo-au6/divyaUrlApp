<h1>Url Shinker</h1>
        <form action="/Shortner" method="POST" class="form-inline my-4">
        <label for="fullUrl" class="sr-only">url</label>
        <input class="form-control col mr-2" name="fullUrl" type="url" placeholder="Url">
        <button class="btn btn-success">Shrink</button>
        </form>
        <table class="table table-striped table-responsive">
            <thead>
                <tr>
                    <th>Full Url</th>
                    <th>Short Url</th>
                    <th>Clicks</th>
                </tr>
            </thead>
            <tbody>
                {{# each shortUrl}}
                <tr>
                    
                    <td><a href="{{this.full}}">{{this.full}}</a></td>
                    <td><a href="{{this.short}}">{{this.short}}</a></td>
                    <td>{{this.clicks}}</td>
                    
                </tr>
                {{/each}}

            </tbody>

        </table>